{
  "compilerOptions": {
    "moduleResolution": "bundler",
    "target": "ESNext",
    "module": "ESNext",
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "checkJs": true,
    "paths": {
      "$frizzante/*": [
        "./.frizzante/vite-project/lib/*"
      ],
      "$lib/*": [
        "./lib/*"
      ]
    }
  },
  "include": [
    "./.frizzante/vite-project/**/*.svelte",
    "./.frizzante/vite-project/**/*.d.ts",
    "./.frizzante/vite-project/**/*.d.ts",
    "./.frizzante/vite-project/**/*.js",
    "./lib/**/*.svelte",
    "./lib/**/*.d.ts",
    "./lib/**/*.d.ts",
    "./lib/**/*.js"
  ]
}
